= simple_form_for @funcao, :html => { :class => 'form-horizontal' } do |f|
  = f.input :tipo
  = f.error_span :tipo
  = f.input :salario
  = f.error_span :salario
  = f.input :ch
  = f.error_span :ch
  = f.button :submit, :class => 'btn-primary'
  = link_to t('.cancel', :default => t("helpers.links.cancel")), funcaos_path, :class => 'btn btn-default'
